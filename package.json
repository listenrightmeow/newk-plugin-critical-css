{
  "name": "@listenrightmeeow/newk-plugin-critical-css",
  "version": "1.0.1",
  "description": "Intelligent critical CSS extraction and inlining for instant page renders",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo \"Tests coming soon\"",
    "lint": "echo \"Linting coming soon\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "newk",
    "critical-css",
    "performance",
    "css-optimization",
    "first-paint",
    "above-the-fold",
    "css-inlining",
    "pagespeed"
  ],
  "author": "@listenrightmeeow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/listenrightmeow/newk-plugin-critical-css.git"
  },
  "bugs": "https://github.com/listenrightmeow/newk-plugin-critical-css/issues",
  "homepage": "https://github.com/listenrightmeow/newk-plugin-critical-css#readme",
  "newk": {
    "type": "optimization",
    "category": "performance",
    "priority": 9
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "puppeteer": "^22.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^24.2.1",
    "rimraf": "^5.0.0",
